include adan.io;
include adan.system;
include adan.string;

program::int main() {
    exec("clear");

    set_seed_as_current_time();

    while (true) {
        print("> ");

        cmd::string = readln();

        if (starts_with(cmd, "exit") == 1) {
            break;
        }

        exec(cmd);
    }

    return 0;
}